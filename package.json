{
  "name": "asyncink",
  "version": "1.1.1",
  "description": "Mikhail T. personal web-site",
  "author": "Mikhail T. <totalism@icloud.com>",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/asyncink/asyncink.git"
  },
  "scripts": {
    "clean": "rm -rf node_modules .next && yarn",
    "start": "next dev",
    "start:prod": "next start",
    "build": "next build",
    "postbuild": "next-sitemap",
    "format": "prettier --write . && eslint '*.js' 'src/**/*.{js,ts,tsx}' --fix",
    "checkup": "yarn lint && yarn typecheck",
    "lint": "next lint && stylelint '**/*.css'",
    "typecheck": "tsc --pretty --noEmit",
    "release": "standard-version",
    "postrelease": "git push --follow-tags",
    "postinstall": "husky install",
    "precommit": "lint-staged"
  },
  "dependencies": {
    "clsx": "^1.2.1",
    "date-fns": "^2.29.3",
    "next": "^13.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.2.0",
    "@commitlint/config-conventional": "^17.2.0",
    "@svgr/webpack": "^6.5.1",
    "@types/marked": "^4.0.7",
    "@types/node": "^18.11.9",
    "@types/react": "^18.0.24",
    "@types/react-dom": "^18.0.8",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "eslint": "^8.26.0",
    "eslint-config-next": "^13.0.2",
    "eslint-config-prettier": "^8.5.0",
    "gray-matter": "^4.0.3",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "marked": "^4.2.1",
    "next-sitemap": "^3.1.30",
    "postcss": "^8.4.18",
    "postcss-preset-env": "^7.8.2",
    "prettier": "^2.7.1",
    "standard-version": "^9.5.0",
    "stylelint": "^14.14.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^29.0.0",
    "typescript": "^4.8.4"
  }
}
