{
  "name": "asyncink",
  "version": "1.0.2",
  "description": "Mikhail T. personal web-site",
  "author": "Mikhail T. <totalism@icloud.com>",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/asyncink/asyncink.git"
  },
  "scripts": {
    "clean": "rm -rf node_modules .next && yarn",
    "start": "next dev",
    "start:prod": "next start",
    "build": "next build",
    "postbuild": "next-sitemap",
    "format": "prettier --write . && eslint '*.js' 'src/**/*.{js,ts,tsx}' --fix",
    "checkup": "yarn lint && yarn typecheck",
    "lint": "next lint && stylelint '**/*.css'",
    "typecheck": "tsc --pretty --noEmit",
    "test": "jest --passWithNoTests",
    "release": "standard-version",
    "postrelease": "git push --follow-tags",
    "postinstall": "husky install",
    "precommit": "lint-staged"
  },
  "dependencies": {
    "classnames": "^2.3.1",
    "dayjs": "^1.10.7",
    "next": "^12.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.1.0",
    "@commitlint/config-conventional": "^16.0.0",
    "@svgr/webpack": "^6.2.1",
    "@types/node": "^17.0.21",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "eslint": "^8.10.0",
    "eslint-config-next": "^12.0.10",
    "eslint-config-prettier": "^8.4.0",
    "gray-matter": "^4.0.3",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.4",
    "next-sitemap": "^2.5.1",
    "postcss": "^8.4.7",
    "postcss-preset-env": "^7.4.1",
    "prettier": "^2.5.1",
    "remark": "^14.0.2",
    "remark-highlight.js": "^7.0.1",
    "remark-html": "^15.0.1",
    "remark-slug": "^7.0.1",
    "remark-unwrap-images": "^3.0.1",
    "stylelint": "^14.5.3",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^25.0.0",
    "typescript": "^4.5.5"
  }
}
