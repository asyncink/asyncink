{
  "name": "asyncink",
  "version": "0.0.0",
  "description": "Mikhail T. personal web-site",
  "author": "Mikhail T. <totalism@icloud.com>",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/totalism/asyncink.git"
  },
  "scripts": {
    "dev": "code . && next dev",
    "build": "next build",
    "start": "next start",
    "checkup": "yarn lint && yarn typecheck",
    "lint": "eslint . --ext ts --ext tsx --ext js && stylelint '**/*.css'",
    "format": "prettier --write . && eslint . --ext ts --ext tsx --ext js --fix",
    "typecheck": "tsc --pretty --noEmit",
    "precommit": "lint-staged",
    "preversion": "echo \"Running tests for version $npm_package_version...\" && yarn test",
    "postversion": "git push --follow-tags && echo \"Successfully released version $npm_package_version!\""
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "dayjs": "^1.10.4",
    "gray-matter": "^4.0.2",
    "next": "latest",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "remark": "^13.0.0",
    "remark-autolink-headings": "^6.0.1",
    "remark-highlight.js": "^6.0.0",
    "remark-html": "^13.0.1",
    "remark-slug": "^6.0.0"
  },
  "devDependencies": {
    "@svgr/webpack": "^5.5.0",
    "@types/node": "^14.14.25",
    "@types/react": "^17.0.1",
    "@types/react-dom": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^4.14.2",
    "@typescript-eslint/parser": "^4.14.2",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "identity-obj-proxy": "^3.0.0",
    "lint-staged": "^10.0.10",
    "postcss-nested": "^5.0.3",
    "prettier": "^2.0.2",
    "stylelint": "^13.10.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^20.0.0",
    "typescript": "^4.1.3"
  }
}
