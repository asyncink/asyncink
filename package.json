{
  "name": "asyncink",
  "version": "1.0.1",
  "description": "Mikhail T. personal web-site",
  "author": "Mikhail T. <totalism@icloud.com>",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/asyncink/asyncink.git"
  },
  "scripts": {
    "start": "next dev",
    "build": "next build",
    "start:prod": "next start",
    "checkup": "yarn lint && yarn typecheck",
    "lint": "eslint . --ext ts --ext tsx --ext js && stylelint '**/*.css'",
    "format": "prettier --write . && eslint . --ext ts --ext tsx --ext js --fix",
    "typecheck": "tsc --pretty --noEmit",
    "precommit": "lint-staged",
    "preversion": "echo \"Linting and type checking version $npm_package_version...\" && yarn checkup",
    "postversion": "git push --follow-tags && echo \"Successfully released version $npm_package_version!\""
  },
  "dependencies": {
    "classnames": "^2.3.1",
    "dayjs": "^1.10.4",
    "next": "10.2.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@svgr/webpack": "^5.5.0",
    "@types/node": "^15.3.0",
    "@types/react": "^17.0.5",
    "@types/react-dom": "^17.0.5",
    "@typescript-eslint/eslint-plugin": "^4.23.0",
    "@typescript-eslint/parser": "^4.23.0",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "gray-matter": "^4.0.3",
    "identity-obj-proxy": "^3.0.0",
    "lint-staged": "^11.0.0",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.3.0",
    "remark": "^13.0.0",
    "remark-autolink-headings": "^6.0.1",
    "remark-highlight.js": "^6.0.0",
    "remark-html": "^13.0.1",
    "remark-slug": "^6.0.0",
    "remark-unwrap-images": "^2.0.0",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^22.0.0",
    "typescript": "^4.2.4"
  }
}
